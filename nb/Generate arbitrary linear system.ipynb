     "output_type": "execute_result"
    }
   ],
   "source": [
    "function gen_Axb(n)\n",
    "    a = rand(n)\n",
    "    A = [ones(n) a]\n",
    "    e = randn(Float64, n)\n",
    "#    display(e)\n",
    "    x = [1,2]\n",
    "    b = A*x + e\n",
    "    return A, x, b\n",
    "end"
   ]
