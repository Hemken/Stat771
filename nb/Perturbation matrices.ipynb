   "source": [
    "# Perturbed Matrix\n",
    "\n",
    "(The ordering of these perturbations seems to matter.)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "pertmat (generic function with 1 method)"
      ]
     },
     "execution_count": 12,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "function pertmat(n,m, delta)\n",
    "    P = zeros(n,m)\n",
    "    P[1,:] = repeat(1:1, inner=m)\n",
    "    for i = 1:(min(n,m)-1)\n",
    "        P[i+1,i] = delta\n",
    "    end\n",
    "    return P\n",
    "end"
   ]
  },
